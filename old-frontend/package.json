{
    "name": "bluestone-frontend",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "build": "cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.prod.ts",
        "start": "cross-env NODE_ENV=development webpack serve --config ./config/webpack/webpack.dev.ts",
        "start:prod": "cross-env NODE_ENV=production webpack serve --config ./config/webpack/webpack.prod.ts",
        "start:test": "cross-env NODE_ENV=test webpack serve --config ./config/webpack/webpack.test.ts",
        "________________________________Project linting_____________________________": "",
        "lint": "eslint \"**/*.{js,ts,tsx}\"",
        "lint:types": "tsc --noEmit",
        "________________________________Project cleaning____________________________": "",
        "format": "prettier --write \"**/*.{js,ts,tsx}\"",
        "clean": "rm -rf dist prodBundleReport.html",
        "clean:all": "npm run clean && rm -rf node_modules",
        "________________________________Project testing_____________________________": "",
        "test": "jest --config=./config/jest/jest.config.ts",
        "test:watch": "npm run test -- --watch",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "cypress:run:spec": "npm run cypress:run -- --headless --spec \"cypress/integration/sample_spec.ts\"",
        "cypress:start": "start-server-and-test start:test http://localhost:3000 cypress:run:spec"
    },
